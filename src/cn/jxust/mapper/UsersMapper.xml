<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="UnionBankMapper">
	<insert id="saveStat">
	   	insert into stat_union_bank(bank_card_no,bank_type,house_holder,terminalType,terminalVersion,create_time)
	   	values(#{bankCardNo},#{bankType},#{houseHolder},#{terminalType},#{terminalVersion},NOW())
	 </insert>
	
</mapper>