<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="FileMapper">
	
    <insert id="saveFile">
    	insert ignore into 
    	bank_file(file_name,district_file_path,account,time,bank_type)
    	values(#{fileName},#{path},#{loginName},NOW(),#{bankType})
    </insert>
    
</mapper>